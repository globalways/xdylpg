<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="psu" class="com.xdylpg.ORM.Psu">
		<primary-key>
			<generator class="com.xdylpg.util.ShopIDGenerator"></generator>
		</primary-key>
	</table>
	<table name="buscat"
		class="com.xdylpg.ORM.Category">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="busadm"
		class="com.xdylpg.ORM.BusAdmin">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="shop" class="com.xdylpg.ORM.Shop">
		<primary-key>
			<generator class="com.xdylpg.util.ShopIDGenerator"></generator>
		</primary-key>
	</table>
	<table name="cbinfo"
		class="com.xdylpg.ORM.CBInfo">
		<primary-key>
			<generator class="com.xdylpg.util.ShopIDGenerator"></generator>
		</primary-key>
	</table>
	<table name="shoppro"
		class="com.xdylpg.ORM.Shoppro">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="auth" class="com.xdylpg.ORM.Auth">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="unitimg"
		class="com.xdylpg.ORM.UnitImg">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="comconsor"
		class="com.xdylpg.ORM.Comconsor">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="cpcontent"
		class="com.xdylpg.ORM.Cpcontent">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="application" class="Application">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="advice">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="vcode" class="Vcode">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="owner" class="Owner">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>